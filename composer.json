{
    "name": "project-ufa/yii2-srbac",
    "description": "A web interface for administration of rbac (CDbAuthManager)",
    "type": "yii2-extension",
    "keywords": ["yii2","srbac"],
    "license": "BSD-3-Clause",
    "support": {
      "issues": "https://github.com/project-ufa/yii2-srbac/issues?state=open",
      "wiki": "https://github.com/project-ufa/yii2-srbac/wiki",
      "source": "https://github.com/project-ufa/yii2-srbac"
    },
     "require": {
        "yiisoft/yii2": "*"
    },
    "autoload": {
        "psr-4": {
            "projectufa\\srbac\\": ""
        }
    },
    "repositories":[
      {
        "type": "git",
        "url": "https://github.com/project-ufa/yii2-srbac"
      }
    ]
}
